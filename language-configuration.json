{
    "comments": {
        "lineComment": "//",
    },
    "brackets": [
        [
            "{{",
            "}}"
        ],
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        [
            "{{",
            "}}"
        ],
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "wordPattern": "[a-zA-Z_0-9\\$\\-]+",
    "indentationRules": {
        "increaseIndentPattern": "^.*(\\{|\\{\\{)[^}\"']*$",
        "decreaseIndentPattern": "^\\s*\\}"
    }
}